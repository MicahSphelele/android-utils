<?xml version="1.0" encoding="utf-8"?>
<resources>
        <declare-styleable name="CountryCodePicker">
            <attr format="integer" name="picker_defaultCode"/>
            <attr format="color" name="picker_textColor"/>
            <attr format="color" name="picker_backgroundColor"/>
            <attr format="dimension" name="picker_textSize"/>
            <attr format="string" name="picker_textFont"/>
            <attr format="dimension" name="picker_arrowSize"/>
            <attr format="string" name="picker_countryPreference"/>
            <attr format="string" name="picker_defaultNameCode"/>
            <attr format="boolean" name="picker_hideNameCode"/>
            <attr format="boolean" name="picker_keyboardAutoPopOnSearch"/>
            <attr format="string" name="picker_customMasterCountries"/>
            <attr format="boolean" name="picker_showFlag"/>
            <attr format="boolean" name="picker_showFullName"/>
            <attr format="boolean" name="picker_selectionDialogShowSearch"/>
            <attr format="boolean" name="picker_clickable"/>
            <attr format="boolean" name="picker_hidePhoneCode"/>
            <attr format="boolean" name="picker_enableHint"/>
            <attr format="boolean" name="picker_enablePhoneAutoFormatter"/>
            <attr format="boolean" name="picker_setCountryByTimeZone"/>
            <!--For dialog-->
            <attr format="color" name="pcp_dialogTextColor"/>
        </declare-styleable>
</resources>